{
    "extends": ["prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error"]
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
          "modules": true
        },
        "requireConfigFile": false,
        "babelOptions": {
          "presets": ["@babel/preset-react"]
       }
    },
    "env": {
        "es6": true
    },
    "parser": "@babel/eslint-parser"
}
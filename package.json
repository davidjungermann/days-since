{
  "homepage": "https://davidjungermann.github.io/days-since/",
  "name": "days-since",
  "version": "0.1.0",
  "private": true,
  "file_pattern": "{src,functions}/**/*.{js,jsx}",
  "engines": {
    "node": "16"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "bootstrap": "5.1.3",
    "emojisplosion": "^2.6.1",
    "firebase": "^9.1.2",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^4.1.1",
    "firebase-tools": "^11.20.0",
    "history": "^5.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.2",
    "react-dom": "^17.0.2",
    "react-firebase-hooks": "^3.0.4",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "prettier-check": "prettier --config .prettierrc ${npm_package_file_pattern} --check",
    "prettier-format": "prettier --config .prettierrc ${npm_package_file_pattern} --write",
    "lint-check": "eslint ${npm_package_file_pattern}",
    "lint-format": "eslint ${npm_package_file_pattern} --fix",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "firebase-functions-test": "^0.2.0",
    "gh-pages": "^3.2.3",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.5.1"
  },
  "main": "functions/index.js",
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged --pattern ${npm_package_file_pattern}"
    }
  }
}
